<script type="text/javascript">
$(document).ready(function(){
	$("#username").blur(function(){
		var name=$("#username").val();
		if(name==""){
			$("#sp").html("用户名不能为空");	
		}
	});
	
	$("#pwd").blur(function(){
		var name=$("#pwd").val();
		if(name==""){
			$("#sp1").html("密码不能为空");	
		}
	});
});

</script>
